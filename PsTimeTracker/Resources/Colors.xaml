<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:local="clr-namespace:PhotoshopTimeCounter">


    <Color x:Key="MainBorder">#353535</Color>
    <SolidColorBrush x:Key="MainBorderBrush" Color="{StaticResource MainBorder}"/>
    
    <Color x:Key="MainForeground">#252525</Color>
    <SolidColorBrush x:Key="MainForegroundBrush" Color="{StaticResource MainForeground}"/>

</ResourceDictionary>